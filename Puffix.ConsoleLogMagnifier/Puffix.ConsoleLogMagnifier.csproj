<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Key.pfx</AssemblyOriginatorKeyFile>
    <Authors>Romain Ehret</Authors>
    <Product>ConsoleLogMagnifier</Product>
    <Company>Romain Ehret</Company>
    <Copyright>Copyright 2018 (c) Romain Ehret. All rights reserved.</Copyright>
    <Description>Magnify your log in your Console App, int .Net and .Net Core.</Description>
    <PackageTags>Console Log .Net Core</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/EhRom/ConsoleLogMagnifier</RepositoryUrl>
    <PackageProjectUrl>https://github.com/EhRom/ConsoleLogMagnifier</PackageProjectUrl>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <PackageReleaseNotes>Rename WriteLine to WriteNewLine to avoid confusions with the original WriteLine method</PackageReleaseNotes>
    <Version>1.1.0</Version>
  </PropertyGroup>

</Project>
